{
  "name": "canvas-carousel",
  "version": "0.0.1",
  "description": "Canvas Carousel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanahawthorne/canvas-carousel.git"
  },
  "scripts": {
    "build": "esbuild --bundle ./src/main.ts --outdir=dist --minify",
    "build:dev": "esbuild --bundle ./src/main.ts --outdir=dist --servedir=dist",
    "watch-ts": "webpack --config webpack.config.js --mode production"
  },
  "browserslist": [
    "Opera >= 12"
  ],
  "dependencies": {
    "esbuild": "^0.14.34"
  },
  "devDependencies": {
    "@webpack-cli/init": "^1.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.22.8",
    "html-webpack-plugin": "^5.5.0",
    "promise": "^8.1.0",
    "promise-polyfill": "^8.2.3",
    "promise.prototype.finally": "^3.1.3",
    "serve": "^13.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "whatwg-fetch": "^3.6.2"
  }
}
